DATABASE ../database/sit.dat
SELECTED_OUTPUT 101
    -file                 test_sit_101.sel
USER_PUNCH 101
    -headings Mu SC
    -start
10 x = MU
20 GOSUB 1000
30 PUNCH STR_F$(round, 12, 9)
110 x = SC
120 GOSUB 1000
130 PUNCH STR_F$(round, 12, 3)
999 END
1000 REM Rounding function 
1010 str_exp$ = STR_E$(x, 10, 3)
1020 round = VAL(str_exp$)
1040 RETURN
    -end
#KNOBS
#	-debug_model
SOLUTION
	-units mol/kgw
	pH	0   charge
	Cl	3
	K       .001
	N(5)    .001