DATABASE ../database/phreeqc.dat
SELECTED_OUTPUT 101
    -file                 dissolve_only_101.sel
USER_PUNCH 101
    -headings Mu SC
    -start
10 x = MU
20 GOSUB 1000
30 PUNCH STR_F$(round, 12, 9)
110 x = SC
120 GOSUB 1000
130 PUNCH STR_F$(round, 12, 3)
999 END
1000 REM Rounding function 
1010 str_exp$ = STR_E$(x, 10, 3)
1020 round = VAL(str_exp$)
1040 RETURN
    -end
SOLUTION 1
EQUILIBRIUM_PHASES 1
	Dolomite	0.0	0.00001 dissolve_only
END
SOLUTION 1
EQUILIBRIUM_PHASES 1
	Dolomite	0.0	0.1 dissolve_only
END
SOLUTION 1
EQUILIBRIUM_PHASES 1
	Dolomite	1.0	0.1 dissolve_only
SAVE solution 2
END
SOLUTION 1
EQUILIBRIUM_PHASES 1
	Dolomite	-1.0	0.1 dissolve_only
SAVE solution 3
END
USE solution 2
EQUILIBRIUM_PHASES 1
	Dolomite	0.0	0.1 dissolve_only
END
USE solution 2
EQUILIBRIUM_PHASES 1
	Dolomite	0.0	0.1 dissolve_only
REACTION
	HCl	1.0
	.5 moles in 5 steps
END
USE solution 3
