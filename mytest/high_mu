database ../database/wateq4f.dat
SELECTED_OUTPUT 101
    -file                 high_mu_101.sel
USER_PUNCH 101
    -headings Mu SC
    -start
10 x = MU
20 GOSUB 1000
30 PUNCH STR_F$(round, 12, 9)
110 x = SC
120 GOSUB 1000
130 PUNCH STR_F$(round, 12, 3)
999 END
1000 REM Rounding function 
1010 str_exp$ = STR_E$(x, 10, 3)
1020 round = VAL(str_exp$)
1040 RETURN
    -end
SOLUTION

	Na	5
	Cl	5
	-units mol/kgw
	pe 10
#	pH -2
#	S  10 charge
END
USE solution 1
REACTION
	H2O 1
	-50 mol
