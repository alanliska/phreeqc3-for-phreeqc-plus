DATABASE ../database/pitzer.dat
SELECTED_OUTPUT 101
    -file                 exch_related_pz_101.sel
USER_PUNCH 101
    -headings Mu SC
    -start
10 x = MU
20 GOSUB 1000
30 PUNCH STR_F$(round, 12, 9)
110 x = SC
120 GOSUB 1000
130 PUNCH STR_F$(round, 12, 3)
999 END
1000 REM Rounding function 
1010 str_exp$ = STR_E$(x, 10, 3)
1020 round = VAL(str_exp$)
1040 RETURN
    -end
solution 1
equilibrium_phases 1
	Calcite	0.0	0.01
save solution 1
save equilibrium_phases 1
end
reaction 1
	HCl	1.0
	0.25	moles
exchange 1
	Ca.5X	Calcite	0.33
	-eq	solution 1
use solution 1
use equilibrium_phases 1
save solution 2
save exchange 2
save equilibrium_phases 2
end
use solution 2
use equilibrium_phases 2
use exchange 2
end
solution 1
equilibrium_phases 1
	Calcite	0.0	0.0
save solution 1
end
