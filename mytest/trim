database ../database/phreeqc.dat
SELECTED_OUTPUT 101
    -file                 trim_101.sel
USER_PUNCH 101
    -headings Mu SC
    -start
10 x = MU
20 GOSUB 1000
30 PUNCH STR_F$(round, 12, 9)
110 x = SC
120 GOSUB 1000
130 PUNCH STR_F$(round, 12, 3)
999 END
1000 REM Rounding function 
1010 str_exp$ = STR_E$(x, 10, 3)
1020 round = VAL(str_exp$)
1040 RETURN
    -end
SOLUTION

USER_PRINT
10 s$ = ""
20 FOR i = 1 to 1000
30    s$ = s$ + '0123456789'
40    PRINT i, "s$ = ", TRIM(s$)
50 NEXT i
