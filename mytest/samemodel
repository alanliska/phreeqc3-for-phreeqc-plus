database ../database/phreeqc.dat
SELECTED_OUTPUT 101
    -file                 samemodel_101.sel
USER_PUNCH 101
    -headings Mu SC
    -start
10 x = MU
20 GOSUB 1000
30 PUNCH STR_F$(round, 12, 9)
110 x = SC
120 GOSUB 1000
130 PUNCH STR_F$(round, 12, 3)
999 END
1000 REM Rounding function 
1010 str_exp$ = STR_E$(x, 10, 3)
1020 round = VAL(str_exp$)
1040 RETURN
    -end
SOLUTION
EQUILIBRIUM_PHASES 1
	CO2(g) -3.0 10
END
EQUILIBRIUM_PHASES 2
	CO2(g) -2.0 10
END
EQUILIBRIUM_PHASES 3
	CO2(g) -1.0 10
END
USE solution 1
USE equilibrium_phases 1
END
USE solution 1
USE equilibrium_phases 2
END
USE solution 1
USE equilibrium_phases 3
END

