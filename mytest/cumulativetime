DATABASE ../database/phreeqc.dat
SELECTED_OUTPUT 101
    -file                 cumulativetime_101.sel
USER_PUNCH 101
    -headings Mu SC
    -start
10 x = MU
20 GOSUB 1000
30 PUNCH STR_F$(round, 12, 9)
110 x = SC
120 GOSUB 1000
130 PUNCH STR_F$(round, 12, 3)
999 END
1000 REM Rounding function 
1010 str_exp$ = STR_E$(x, 10, 3)
1020 round = VAL(str_exp$)
1040 RETURN
    -end
# No incremental reactions, No selected_output
INCREMENTAL_REACTIONS false
SOLUTION
	O(0) 1 O2(g) -0.7
KINETICS 1
Pyrite
    -formula  FeS2  1
    -m        1
    -m0       1
    -parms    2 0.67 0.5 -0.11
    -tol      1e-008
-steps       31500000 in 3 steps # seconds
-step_divide 1
-runge_kutta 3
-bad_step_max 500
END
# Yes incremental reactions, no selected_output
INCREMENTAL_REACTIONS true
USE solution 1
USE kinetics 1
END
# No incremental reactions, selected_output
INCREMENTAL_REACTIONS false
USE solution 1
SELECTED_OUTPUT
	-file cumulativetime.sel
	-reset false
	-time
USE kinetics 1
END
# Yes incremental reactions, selected_output
INCREMENTAL_REACTIONS true 
USE solution 1
INCREMENTAL_REACTIONS true
USE kinetics 1
END