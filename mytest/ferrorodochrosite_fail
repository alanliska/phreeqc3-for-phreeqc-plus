Katalsurf 
    -start
		5  k = 0
           20 if EQUI("Fe_tri(OH)3(a)") > 2.5*0.001869 then  k = 0.00015
           30 if EQUI("Fe_tri(OH)3(a)") <= 0 then goto 50
	     40 moles = 	k*EQUI("Fe_tri(OH)3(a)")*TIME
	     50 SAVE moles
	-end



Fe_di+2 	# реакция гомогенного окисления Fe+2
	-start 
		5 k =7.9e-6  #2e-7*(2.71828^(0.1404*TC))
		10 Fe_di = MOL("Fe_di+2") 
		20 if (Fe_di <= 0) then goto 200 
		100 moles = (k*Fe_di )*MOL("O2")* TIME  
		200 SAVE moles 
	-end 

Fe_di(OH)+	 	#реакция гомогенного окисления Fe(OH)+
	-start 
		5  k = 25.1#10^(1.4)
		10 Fe_diOH = MOL("Fe_diOH+") 
		20 if (Fe_diOH <= 0) then goto 200 
		50 k = 0.7752*(2.71828^(0.1404*TC))
		100 moles = (k*Fe_diOH)*MOL("O2")* TIME  
		200 SAVE moles 
	-end 

Fe_di(OH)2	 	#реакция гомогенного окисления Fe(OH)2
	-start 
		5  k = 7.94e6#10^(6.9)
		10 Fe_diOHdi = MOL("Fe_di(OH)2") 
		20 if (Fe_diOHdi <= 0) then goto 200 
		50 k = 246320*(2.71828^(0.1404*TC))
		100 moles = (k*Fe_diOHdi)*MOL("O2")* TIME  
		200 SAVE moles 
	-end 

Fe_tri-OFe+	 	#реакция гетерогенного окисления сорбированной формы > Fe-OFe+	 
	-start 
		50 k = 5.01 
            60 if ((MOL("Fetri_wOFe_diOH")+MOL("Fetri_sOFe_diOH"))<= 0) then goto 200
		100 moles = (k*(MOL("Fetri_wOFe_diOH")+MOL("Fetri_sOFe_diOH")))*MOL("O2")*TIME 
		200 SAVE moles 
	-end 

Fe_tri-OFeOH	 	##реакция гетерогенного окисления сорбированной формы > Fe-OFeOH	 
	-start 
		50 k = 5.01
		60 if ((MOL("Fetri_wOFe_di+")+MOL("Fetri_sOFe_di+"))<= 0) then goto 200
		100 moles = (k*(MOL("Fetri_wOFe_di+")+MOL("Fetri_sOFe_di+")))*MOL("O2")*TIME  
		200 SAVE moles 
	-end 
Fe_diX2	 	# реакция гетерогенного окисления сорбированной формы FeX2
	-start 

		5 k = 0.3*1.88e-3*((TOT("X"))*1000) 
		10 Fe_diX = MOL("Fe_diX2") 
		20 if (Fe_diX <= 0) then goto 200 
		100 moles = (k*Fe_diX )*MOL("O2")* TIME  
		200 SAVE moles 
	-end 

Ferrihydrite-goethite	 	# переход ферригидрита в гётит
	-start
		10 k = 8.022e-8*2# 
		50 if EQUI("Fe_tri(OH)3(a)") <= 0 then goto 200
		100 moles = (k*EQUI("Fe_tri(OH)3(a)"))* TIME  
		200 SAVE moles 
	-end 


PHASES 
Ferrorhodochrosite
        Mn_diFe_di(CO3)2 +2.0000 H+  =  + 2.0000 HCO3- + 1.0000 Mn_di++ + 1.0000 Fe_di++
        log_k           -1.5852484265323838803134341253664
	-delta_H	-21.3426	kJ/mol	# Calculated enthalpy of reaction	Rhodochrosite
        #-analytic -1.6195e+002 -4.9344e-002 5.0937e+003 6.4402e+001 7.9531e+001



Rhodochrosite
        Mn_diCO3 +1.0000 H+  =  + 1.0000 HCO3- + 1.0000 Mn_di++
        log_k           -0.1928
	-delta_H	-21.3426	kJ/mol	# Calculated enthalpy of reaction	Rhodochrosite
        -analytic -1.6195e+002 -4.9344e-002 5.0937e+003 6.4402e+001 7.9531e+001
Fe_goethite   
	Fe_goethiteOOH + 3H+ = Fe_goethite+3 + 2 H2O   
	log_k -30

Goethite 
	Fe_triOOH + 3 H+ = Fe_tri+3 + 2 H2O 
	log_k -30
	-delta_H -61.9291 kJ/mol 

pH_Fix						
	H+ = H+ 
 	log_k  0  

Katalsurf 
	Katalsurf(OH)3 +3H+ = Katalsurf+3 + 3H2O 
	log_k -70
	

Fe_tri(OH)3(a) 
	Fe_tri(OH)3 +3H+ = Fe_tri+3 + 3H2O 
	log_k 5.6556 
	-delta_H -84.0824 kJ/mol


Manganite
        Mn_triO(OH) +3.0000 H+  =  + 1.0000 Mn_tri+++ + 2.0000 H2O
       log_k           -0.1646
	-delta_H	0	      	

KINETICS 1-20

Fe_tri-OMn_di+
      #-formula  Mn_di  -1.0  O2  -0.25  H+ -1 Mn_tri+3  1.0  H2O 0.5
      -formula  Mn_di  -1.0  Mn_tri  1.0
Mn_di
      #-formula  Mn_di  -1.0  O2  -0.25  H+ -1 Mn_tri+3  1.0  H2O 0.5
      -formula  Mn_di  -1.0  Mn_tri  1.0
Katalsurf 
-formula Fe_tri(OH)3(a) -10
Fe_di+2 
	#-formula 	Fe_di+2 -1.0 	O2 -0.25 	H+ -1   	Fe_tri+3 1.0  	H2O 0.5  #Katalsurf(OH)3 1 
	-formula 	Fe_di -1.0 	Fe_tri 1.0
Fe_di(OH)+ 
	#-formula 	Fe_diOH+ -1.0 	O2 -0.25    H+ -1   	Fe_triOH+2 1.0	H2O 0.5  #Katalsurf(OH)3   1
	-formula 	Fe_di -1.0 	Fe_tri 1.0
Fe_di(OH)2
	#-formula 	Fe_di(OH)2 -1.0 	O2 -0.25    H+ -1 	Fe_tri(OH)2+ 1.0 	H2O 0.5 #Katalsurf(OH)3   1 
	-formula 	Fe_di -1.0 	Fe_tri 1.0
Fe_tri-OFe+
	#-formula 	Fe_di -1.0   	O2 -0.25  	H+ -1    	Fe_tri+3    +1	H2O 0.5 #Katalsurf(OH)3   1
	-formula 	Fe_di -1.0 	Fe_tri 1.0
Fe_tri-OFeOH
	#-formula 	Fe_di -1.0 		O2 -0.25	H+ -1       Fe_tri+3  +1.0   	H2O 0.5  #Katalsurf(OH)3   1
	-formula 	Fe_di -1.0 	Fe_tri 1.0
Fe_diX2
	#-formula 	Fe_diX2 -1.0 	O2 -0.25 	H+ -1   	Fe_tri+3 1.0  	H2O 0.5 	#Katalsurf(OH)3   1
	-formula 	Fe_di -1.0 	Fe_tri 1.0
Ferrihydrite-goethite
	-formula 	Fe_tri(OH)3  -1.0     Fe_goethite 1.0 		H2O 1.0 		#Katalsurf(OH)3   -1
Mn_tri-Fedi_ox
	-formula  Mn_tri -1 Fe_di -1 Mn_di 1 Fe_tri 1



SURFACE 1-20
	Fetri_w Fe_tri(OH)3(a) equilibrium_phase 0.21 64200 
	Fetri_s Fe_tri(OH)3(a) equilibrium_phase 0.0054			
       -equilibrate 1


EQUILIBRIUM_PHASES 1-20
