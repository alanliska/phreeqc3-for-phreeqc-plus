RATES # 




Mn_di
 -start
5   k = 50.1
10  Mn_di = MOL("Mn_di(OH)2")
20  if (Mn_di <= 0) then goto 200
40  moles =  Mn_di* k* MOL("O2")* TIME
200 SAVE moles
-end

Mn_tri-Fedi_ox
 -start
5   k = 1^6
10  Mn_tri = MOL("Mn_tri+3")
20  if (Mn_tri <= 0) then goto 200      #!
40  moles =  Mn_tri*TOT("Fe_di")*TIME
200 SAVE moles
-end


Fe_tri-OMn_di+
-start 
50 k = 10^(-0.16)
60 if ((MOL("Fetri_wOMn_di+")+MOL("Fetri_sOMn_di+"))<=0) then goto 200
100 moles = (k*(MOL("Fetri_wOMn_di+")+MOL("Fetri_sOMn_di+")))*MOL("O2")*TIME # 
200 SAVE moles 
-end 


