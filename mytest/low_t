DATABASE ../database/iso.dat
SELECTED_OUTPUT 101
    -file                 low_t_101.sel
USER_PUNCH 101
    -headings Mu SC
    -start
10 x = MU
20 GOSUB 1000
30 PUNCH STR_F$(round, 12, 9)
110 x = SC
120 GOSUB 1000
130 PUNCH STR_F$(round, 12, 3)
999 END
1000 REM Rounding function 
1010 str_exp$ = STR_E$(x, 10, 3)
1020 round = VAL(str_exp$)
1040 RETURN
    -end
SOLUTION 1
	temp 25
	[18O] 0
END
SOLUTION_MODIFY 1
	-temp -22.5
	-total
		[18O]  .1062
END
#knobs
#-debug_model
MIX 
	1 1
END
