USE solution 1
REACTION_TEMPERATURE; 12.500000
REACTION_PRESSURE; 1.000000
# from 0 to 10.
REACTION; AmmHCl 1
0 0.001000 0.002000 0.005000 0.010010 0.020020 0.050060 0.100130 0.200260 0.500650 0.600780
#USER_PUNCH
USER_GRAPH; -headings 12.C1.atm
 -axis_titles "NH4Cl / (mol/kg H2O)" "NH4Cl molar volume / (cm3/mol)"
10 data 0, 34.510000, 33.509000, 35.508000, 35.523000, 34.517000, 34.701000, 35.071000, 35.959000, 36.529000, 36.418000,
20 dim d(11)
30 read d(1), d(2), d(3), d(4), d(5), d(6), d(7), d(8), d(9), d(10), d(11),
40 if step_no = 1 then put(rho, 10)
50 if step_no > 1 then Vm1 = (1e3 + tot("Cl") * 53.4917) / (tot("Cl") * rho) - 1e3 / (tot("Cl") * get(10))
60 if step_no = 1 then dif = 0 else dif = Vm1 - d(step_no)
62 if tot("Cl") > 10 or tc > 200 or pressure > 1e3 then dif = 0
#80 punch dif, str$(tc), str$(pressure), str$(step_no + get(1))
90 if step_no = 11 then put(step_no + get(1), 1)
100 if step_no = 1 then goto 150
110 plot_xy tot("Cl"), d(step_no), symbol = Square, line_w = 0, color = Red
120 plot_xy tot("Cl"), Vm1, symbol = None, color = Red
150 end
END

